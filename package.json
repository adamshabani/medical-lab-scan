  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/theomaro/tailwind-typescript-starter/issues"
  },
  "homepage": "https://github.com/theomaro/tailwind-typescript-starter#readme",
  "dependencies": {
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/node": "20.11.5"
  }
}
